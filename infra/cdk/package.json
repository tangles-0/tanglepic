{
  "name": "latex-cdk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "synth": "cdk synth",
    "deploy:dev": "cdk deploy latex-dev-app latex-dev-observability --exclusively -c env=dev",
    "deploy:prod": "cdk deploy latex-prod-app latex-prod-observability --exclusively -c env=prod",
    "deploy:all:dev": "cdk deploy --all -c env=dev",
    "deploy:all:prod": "cdk deploy --all -c env=prod",
    "diff:dev": "cdk diff --all -c env=dev",
    "diff:prod": "cdk diff --all -c env=prod"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.179.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "aws-cdk": "^2.1010.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}

